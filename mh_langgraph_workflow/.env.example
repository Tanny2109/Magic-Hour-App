# API Keys (required)
FAL_KEY=your_fal_api_key
PRODIA_KEY=your_prodia_api_key

# LLM Model via fal.ai any-llm API
# Available models: google/gemini-2.5-flash, anthropic/claude-3.5-sonnet,
#                   openai/gpt-4o, meta-llama/llama-3.1-70b-instruct, etc.
FAL_MODEL_NAME=google/gemini-2.5-flash
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=4096

# Server Settings
PORT=8000
PRODUCTION=false
